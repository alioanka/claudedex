# DexScreener Trading Bot Environment Variables
# Copy this file to .env and fill in your actual values

# ==============================================================================
#  SENSITIVE CREDENTIALS - DO NOT COMMIT TO GIT
# ==============================================================================

# Wallet Security
ENCRYPTION_KEY=
PRIVATE_KEY=
SOLANA_PRIVATE_KEY=

# Wallet Addresses
WALLET_ADDRESS=
SOLANA_WALLET=

# Database & Cache
DATABASE_URL=postgresql://bot_user:bot_password@postgres:5432/tradingbot
REDIS_URL=redis://redis:6379/0

# API Keys
GOPLUS_API_KEY=
1INCH_API_KEY=
PARASWAP_API_KEY=
TWITTER_API_KEY=
TWITTER_API_SECRET=
TWITTER_BEARER_TOKEN=

# Notification Services
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
DISCORD_WEBHOOK_URL=

# ==============================================================================
# RPC ENDPOINTS - Sensitive as they may be personal paid plans
# ==============================================================================

# EVM Chains
ETHEREUM_RPC_URLS=
BSC_RPC_URLS=
POLYGON_RPC_URLS=
ARBITRUM_RPC_URLS=
BASE_RPC_URLS=

# Solana
SOLANA_RPC_URLS=

# Legacy (optional, used as fallback)
WEB3_PROVIDER_URL=

# ==============================================================================
# CORE SETTINGS - Configure via environment to override coded defaults
# ==============================================================================

# Set to 'false' for live trading
DRY_RUN=true

# Comma-separated list of chains to activate (e.g., "ethereum,solana,base")
ENABLED_CHAINS=ethereum,bsc,base,arbitrum,solana
