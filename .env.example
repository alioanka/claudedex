# DexScreener Trading Bot Environment Variables
# Copy this file to .env and fill in your actual values

# Mode Configuration
MODE=production  # development, testing, production
DRY_RUN=true    # IMPORTANT: Set to false ONLY when ready for live trading! Default is paper trading for safety.

# Database Configuration
DATABASE_URL=postgresql://postgres:your_password@localhost:5432/trading_bot
DB_PASSWORD=your_secure_password
REDIS_URL=redis://localhost:6379/0

# Web3 Configuration
WEB3_PROVIDER_URL=https://eth-mainnet.g.alchemy.com/v2/your_alchemy_key
WEB3_BACKUP_PROVIDER_1=https://mainnet.infura.io/v3/your_infura_key
WEB3_BACKUP_PROVIDER_2=https://ethereum.publicnode.com
CHAIN_ID=1  # 1=Ethereum, 56=BSC, 137=Polygon, 42161=Arbitrum, 8453=Base

# IMPORTANT: Private Key Security
# Use a dedicated wallet for the bot, not your main wallet
# Consider using a hardware wallet or key management service in production
PRIVATE_KEY=your_wallet_private_key_encrypted
ENCRYPTION_KEY=your_encryption_key_for_private_key

# API Keys - Data Sources
DEXSCREENER_API_KEY=your_dexscreener_api_key
GOPLUS_API_KEY=your_goplus_api_key
TOKENSNIFFER_API_KEY=your_tokensniffer_api_key

# DEX Aggregator APIs (Optional but recommended)
1INCH_API_KEY=your_1inch_api_key
PARASWAP_API_KEY=your_paraswap_api_key

# Social Media APIs (Optional)
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_BEARER_TOKEN=your_twitter_bearer_token

# Telegram Notifications (Required for alerts)
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# Discord Notifications (Optional)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook

# Email Notifications (Optional)
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
EMAIL_FROM=your_email@gmail.com
EMAIL_TO=recipient@gmail.com
EMAIL_PASSWORD=your_email_app_password

# ToxiSol Configuration (Optional)
TOXISOL_BOT_TOKEN=your_toxisol_bot_token
TOXISOL_API_ENDPOINT=https://api.toxisol.com

# MEV Protection
FLASHBOTS_RPC=https://rpc.flashbots.net
FLASHBOTS_SIGNING_KEY=your_flashbots_signing_key

# Dashboard Configuration
DASHBOARD_API_KEY=generate_secure_api_key_here
DASHBOARD_PORT=8080

# Grafana Monitoring
GRAFANA_PASSWORD=your_grafana_admin_password
PROMETHEUS_PORT=9090

# pgAdmin (Development)
PGADMIN_EMAIL=admin@example.com
PGADMIN_PASSWORD=your_pgadmin_password

# Jupyter (Development)
JUPYTER_TOKEN=your_jupyter_token

# Security Settings
SESSION_SECRET=generate_random_secret_here
JWT_SECRET=generate_random_jwt_secret_here
API_KEY=your_api_key_for_external_access

# Performance Settings
MAX_WORKERS=4
BATCH_SIZE=100
CACHE_TTL=300

# Risk Management Overrides (Optional)
MAX_POSITION_SIZE_PERCENT=5
MAX_DAILY_LOSS_PERCENT=10
MAX_DRAWDOWN_PERCENT=25

# Logging
LOG_LEVEL=INFO  # DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_FILE=/app/logs/trading_bot.log

# Sentry Error Tracking (Optional)
SENTRY_DSN=https://your_sentry_dsn@sentry.io/project_id

# Feature Flags
ENABLE_EXPERIMENTAL_FEATURES=false
USE_AI_SENTIMENT=false
USE_WHALE_TRACKING=true
USE_MEMPOOL_MONITORING=true

# =============================================================================
# FUTURES MODULE CONFIGURATION
# =============================================================================
# Enable/Disable the Futures Trading Module
FUTURES_MODULE_ENABLED=true

# Exchange Selection: binance or bybit
FUTURES_EXCHANGE=binance

# Network Mode: true = testnet (paper trading on exchange), false = mainnet
FUTURES_TESTNET=true

# Trading Pairs (comma-separated)
# Available pairs: BTC/USDT, ETH/USDT, SOL/USDT, BNB/USDT, XRP/USDT, ADA/USDT,
#                  DOGE/USDT, MATIC/USDT, DOT/USDT, AVAX/USDT, LINK/USDT, etc.
# Add more pairs to increase trading opportunities!
FUTURES_SYMBOLS=BTC/USDT,ETH/USDT,SOL/USDT,BNB/USDT

# Position Configuration
FUTURES_POSITION_SIZE_USD=100      # USD value per position
FUTURES_LEVERAGE=10                # Leverage multiplier (1-125)
FUTURES_MAX_POSITIONS=5            # Maximum concurrent open positions

# Risk Management
FUTURES_STOP_LOSS_PCT=-5.0         # Stop loss percentage (applied with leverage)
FUTURES_TAKE_PROFIT_PCT=10.0       # Take profit percentage (applied with leverage)
FUTURES_MAX_DAILY_LOSS_USD=500     # Max daily loss before trading pauses

# =============================================================================
# STRATEGY PARAMETERS - Tune these to find more opportunities!
# =============================================================================
# RSI Thresholds (default is STRICT - change for more signals)
# CONSERVATIVE (default): RSI < 30 = BUY, RSI > 70 = SELL (rarely triggers)
# MODERATE:               RSI < 35 = BUY, RSI > 65 = SELL
# AGGRESSIVE:             RSI < 40 = BUY, RSI > 60 = SELL (frequent signals)

FUTURES_RSI_OVERSOLD=30            # Below this = STRONG_BUY (default: 30)
FUTURES_RSI_OVERBOUGHT=70          # Above this = STRONG_SELL (default: 70)
FUTURES_RSI_WEAK_OVERSOLD=40       # Below this = BUY (default: 40)
FUTURES_RSI_WEAK_OVERBOUGHT=60     # Above this = SELL (default: 60)

# Minimum Signal Score to Enter Trade
# Signal score = RSI_signal + MACD_signal + Volume_signal
# Each indicator contributes -2 to +2 (total range: -6 to +6)
#
# STRICT (default):    4 = Only enters on STRONG signals (RSI extreme + MACD + Volume)
# MODERATE:            3 = Enters on strong + one confirming indicator
# RELAXED:             2 = Enters on regular BUY/SELL signals
# AGGRESSIVE:          1 = Enters on any slight bias
FUTURES_MIN_SIGNAL_SCORE=4

# Verbose Signal Logging (see why trades are rejected)
# Set to true to see detailed signal analysis in logs
FUTURES_VERBOSE_SIGNALS=true

# =============================================================================
# BINANCE API KEYS (for Futures trading)
# =============================================================================
# Testnet keys (get from https://testnet.binancefuture.com)
BINANCE_TESTNET_API_KEY=your_testnet_api_key
BINANCE_TESTNET_API_SECRET=your_testnet_api_secret

# Mainnet keys (get from https://www.binance.com/en/my/settings/api-management)
BINANCE_API_KEY=your_mainnet_api_key
BINANCE_API_SECRET=your_mainnet_api_secret

# =============================================================================
# BYBIT API KEYS (alternative exchange)
# =============================================================================
# Testnet keys (get from https://testnet.bybit.com)
BYBIT_TESTNET_API_KEY=your_testnet_api_key
BYBIT_TESTNET_API_SECRET=your_testnet_api_secret

# Mainnet keys
BYBIT_API_KEY=your_mainnet_api_key
BYBIT_API_SECRET=your_mainnet_api_secret

# Gas Price Limits (in Gwei)
# CRITICAL: 500 Gwei = $150-300 per transaction! Use 50 Gwei max for safety
MAX_GAS_PRICE=50
PRIORITY_GAS_MULTIPLIER=1.2

# Trading Limits
MIN_BALANCE_ETH=0.1
MIN_TRADE_SIZE_USD=100
MAX_TRADE_SIZE_USD=10000

# Model Training
ML_RETRAIN_INTERVAL_HOURS=24
ML_MIN_CONFIDENCE=0.7

# Backtesting
BACKTEST_START_DATE=2024-01-01
BACKTEST_END_DATE=2024-12-31
BACKTEST_INITIAL_BALANCE=1.0

# Network Settings
HTTP_TIMEOUT=30
WS_PING_INTERVAL=30
MAX_RETRIES=3

# Debug Settings (Development Only)
DEBUG=false
TESTING=false
VERBOSE=false